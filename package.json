{
  "name": "TickTackToe",
  "version": "1.0.0",
  "description": "React/Redux Test project",
  "main": "index.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "nodemon --watch server --watch config --watch src --watch modules --watch client ./dist/www",
    "start:prod": "npm run build && node dist/server.bundle.js",
    "build:client": "webpack",
    "build:server": "webpack --config webpack.server.config.js",
    "build": "npm run build:client && npm run build:server",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js \"test/**/*@(.js|.jsx)\"",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Huffaker/TickTackToe.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Huffaker/TickTackToe/issues"
  },
  "homepage": "https://github.com/Huffaker/TickTackToe#readme",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-watch": "^2.0.2",
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "jsdom": "^9.4.1",
    "mocha": "^3.0.0",
    "nodemon": "^1.10.0",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.12.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "dependencies": {
    "babel-register": "^6.11.6",
    "compression": "^1.6.2",
    "config": "^1.21.0",
    "express": "^4.14.0",
    "if-env": "^1.0.0",
    "immutable": "^3.8.1",
    "node-uuid": "^1.4.7",
    "react": "^15.3.0",
    "react-addons-pure-render-mixin": "^15.3.0",
    "react-addons-test-utils": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "redux": "^3.5.2",
    "serve-static": "^1.11.1",
    "shell-arguments": "^1.1.9",
    "socket.io": "^1.4.8",
    "socket.io-client": "^1.4.8"
  }
}
